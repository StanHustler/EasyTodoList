{
  "name": "EasyTodoList",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Just a Todo List in Chrome",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}